{
  "apiHost":"http://texta-rest",
  "apiBasePath":"/api/v2",
  "apiBasePath2":"/api/v2",
  "logging":true,
  "fileFieldReplace":"texta_filepath",
  "useCloudFoundryUAA":true,
  "uaaConf":{
    "uaaURL":"http://texta-uaa:8080",
    "redirect_uri":"http://texta-rest/api/v2/uaa/callback",
    "client_id":"login",
    "scope":"openid texta.*",
    "admin_scope": "texta.project_admin",
    "response_type":"code",
    "oauth_uri": "http://texta-uaa:8080/uaa/oauth",
    "logout_uri": "http://texta-uaa:8080/uaa/logout.do",
    "authorize_uri": "http://texta-uaa:8080/uaa/oauth/authorize"
  }
}
